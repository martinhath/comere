#!/bin/gnuplot

set style fill solid 0.25 border -1
set style boxplot outliers pointtype 7
set style data boxplot
set size square

set pointsize 0.3

set xtics ('t=1' 1, 't=2' 2, 't=4' 3, 't=8' 4)

set terminal pdf size 10cm,10cm


set title "Timings for EBR (transfer)"
set output 'queue-transfer-ebr.pdf'
plot for [i=1:4] 'queue-transfer-ebr' using (i):i notitle

set title "Timings for crossbeam (transfer)"
set output 'queue-transfer-crossbeam.pdf'
plot for [i=1:4] 'queue-transfer-crossbeam' using (i):i notitle

set title "Timings for HP (transfer)"
set output 'queue-transfer-hp.pdf'
plot for [i=1:4] 'queue-transfer-hp' using (i):i notitle
